programa {
  funcao inicio() {
  real numero_1, numero_2, numero_3
  escreva("Digite seus numeros: \n")
  leia(numero_1, numero_2,numero_3)

  se(numero_1+numero_2+numero_3 > 50){
    escreva(numero_1+numero_2+numero_3," é maior que 50")
  }senao{
    escreva(numero_1+numero_2+numero_3, " é menor que 50")
  }
se(numero_1+numero_2+numero_3 == 50)
escreva("\n",numero_1+numero_2+numero_3, " é igual a 50")
  }
}
